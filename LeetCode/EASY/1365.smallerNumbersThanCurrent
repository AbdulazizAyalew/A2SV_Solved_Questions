class Solution:
    def smallerNumbersThanCurrent(self, nums: List[int]) -> List[int]:
        org_arr = nums[:]
        for i in range(1,len(nums)):
            for j in range(i,0,-1):
                if nums[j] < nums[j-1]:
                    nums[j] , nums[j-1] = nums[j-1] , nums[j]
                else:
                    break
        print(nums)
        res = []
        for i in range(len(org_arr)):
            res.append(nums.index(org_arr[i]))
        
        return res

        
