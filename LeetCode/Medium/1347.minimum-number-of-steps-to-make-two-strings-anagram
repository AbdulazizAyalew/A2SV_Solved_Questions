class Solution:
    def minSteps(self, s: str, t: str) -> int:
        s_count = Counter(s)
        t_count = Counter(t)

        No_steps = 0
        for c in t_count:
            if c in s_count: 
                if t_count[c] > s_count[c]:
                    No_steps += (t_count[c] - s_count[c])
            else:
                No_steps += t_count[c]
        
        return No_steps
